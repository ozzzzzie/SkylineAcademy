@{
    ViewData["Title"] = "Student Dashboard";
}
<br />
<br />
<h1 align="center">Student Dashboard</h1>
<br />
<br />
<h4>Personal Information</h4>
<div class="tile-container">
    <div class="tile">
        <a href="@Url.Action("StudentIndex", "Profile")">
            <br />
            <img src="/images/profile.png" alt="Profile Image" />
            <br /> <br />
            <h5>Profile</h5>
        </a>
    </div>
    <div class="tile">
        <a href="@Url.Action("StudentsGrades", "Grades")">
            <br />
            <img src="/images/grades.png" alt="Grades Image" />
            <br /> <br />
            <h5>Grades</h5>
        </a>
    </div>
    <div class="tile">
        <a href="@Url.Action("StudentTimetable", "Enrollements")">
            <br />
            <img src="/images/timetable.png" alt="Timetable Image" />
            <br /> <br />
            <h5>Timetable</h5>
        </a>
    </div>
    <div class="tile">
        <a href="@Url.Action("StuPrevEnrollements", "Enrollements")">
            <br />
            <img src="/images/prevenrollments.png" alt="Enrollments History Image" />
            <br /> <br />
            <h5>Enrollment History</h5>
        </a>
    </div>
</div>
<br /><br />
<h4>General Information</h4>
<div class="tile-container">
    <div class="tile">
        <a href="@Url.Action("ClassroomTimetable", "Classrooms")">
            <br />
            <img src="/images/classroomavailability.png" alt="Classroom Availability Image" />
            <br /> <br />
            <h5>Classroom Timetables</h5>
        </a>
    </div>
    <div class="tile">
        <a href="@Url.Action("Index", "Courses")">
            <br />
            <img src="/images/courses.png" alt="Courses Image" />
            <br /> <br />
            <h5>Courses</h5>
        </a>
    </div>
    <div class="tile">
        <a href="@Url.Action("Index", "Prerequisites")">
            <br />
            <img src="/images/prerequisite.png" alt="Prerequisites Image" />
            <br /> <br />
            <h5>Prerequisites</h5>
        </a>
    </div>
    <div class="tile-invisible">
    </div>
</div>
<br /><br />
<h4>Contact Information</h4>
<div class="tile-container">
    <div class="tile">
        <a href="@Url.Action("Index", "Teachers")">
