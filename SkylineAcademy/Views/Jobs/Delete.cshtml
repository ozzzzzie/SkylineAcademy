@model SkylineAcademy.Models.Job

@{
    ViewData["Title"] = "Delete";
}

<br />
<h1>Delete</h1>


<h3>Are you sure you want to delete this job?</h3>
<div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Jname)
        </dd>
        <dt class = "col-sm-2">
            Description
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Jdescription)
        </dd>
        <dt class = "col-sm-2">
            Department
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Department.Dname)
        </dd>
    </dl>
    <br />
    <form asp-action="Delete">
        <input type="hidden" asp-for="JobId" /><input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index"><button type="button" class="btn btn-light">Back to List</button></a>
    </form>
</div>
