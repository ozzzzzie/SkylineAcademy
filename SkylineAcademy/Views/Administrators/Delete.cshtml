@model SkylineAcademy.Models.Administrator

@{
    ViewData["Title"] = "Delete";
}
<br />
<h1>Delete</h1>

<h3>Are you sure you want to delete this Administrator?</h3>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            First Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Afname)
        </dd>
        <dt class = "col-sm-2">
            Last Name
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Alname)
        </dd>
        <dt class = "col-sm-2">
            Address
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Aaddress)
        </dd>
        <dt class = "col-sm-2">
            Phone Number
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Aphonenumber)
        </dd>
        <dt class = "col-sm-2">
            Email
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Aemail)
        </dd>
        <dt class = "col-sm-2">
            Job
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Job.Jname)
        </dd>
    </dl>
    <br />
    <form asp-action="Delete">
        <input type="hidden" asp-for="AdministratorId" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index"><button type="button" class="btn btn-light">Back to List</button></a>
    </form>
</div>
